/* pages/integralModule/list/index.wxss */
.container {
  padding: 40rpx;
}
.item {
  padding: 20rpx 0;
  border-bottom: 2rpx solid #E4E4E4;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.item__info {
  color: #333333;
  font-size: 28rpx;
}
.item__info view:last-child {
  font-size: 26rpx;
  color: #999999;
}
.item__integra {
  color: #EC7B7C;
  font-size: 30rpx;
  letter-spacing: 4rpx;
}